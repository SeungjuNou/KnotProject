<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC '-//ibatis.apache.org//DTD Sql Map 2.0//EN' 'http://ibatis.apache.org/dtd/sql-map-2.dtd'>

<sqlMap>

	<typeAlias alias="member" type="model.member.memberVO" />

	<resultMap id="memberRes" class="member">
		<result property="mem_no" column="MEM_NO"/>
		<result property="mem_id" column="MEM_ID"/>
		<result property="mem_pwd" column="MEM_PWD"/>
		<result property="mem_name" column="MEM_NAME"/>
		<result property="mem_phone" column="MEM_PHONE"/>
		<result property="mem_img" column="MEM_IMG"/>
		<result property="memo" column="MEMO"/>
		<result property="mem_area_code" column="MEM_AREA_CODE"/>
		<result property="mem_lev" column="MEM_LEV"/>
		<result property="mem_date" column="MEM_DATE"/>
	</resultMap>

	<sql id="select-all">
		select * from MEMBER
	</sql>

	<select id="selectMemAll" resultMap="memberRes" parameterClass="int">
		<include refid="select-all" />
		ORDER BY MEM_NO DESC
	</select>


</sqlMap>


